{
  "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/admin/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/admin/favicon.ico",
          "/admin/index.html",
          "/admin/manifest.webmanifest",
          "/admin/*.css",
          "/admin/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/admin/assets/**",
          "/admin/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
